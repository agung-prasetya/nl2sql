{
  "entitas": {
    "akun": {
      "id_akun": "primary key",
      "nama_akun": "varchar(100)",
      "jenis_akun": "varchar(50)",
      "saldo_awal": "decimal(18,2)",
      "saldo_akhir": "decimal(18,2)"
    },
    "pengguna": {
      "id_pengguna": "primary key",
      "nama": "varchar(100)",
      "username": "varchar(50)",
      "password": "varchar(50)",
      "peran": "varchar(20)"
    },
    "jurnal_umum": {
      "id_jurnal": "primary key",
      "tanggal": "date",
      "deskripsi": "text",
      "id_pengguna": "foreign key references pengguna(id_pengguna)"
    },
    "detail_jurnal": {
      "id_jurnal": "foreign key references jurnal_umum(id_jurnal)",
      "id_akun": "foreign key references akun(id_akun)",
      "posisi": "varchar(10)",
      "jumlah": "decimal(18,2)",
      "primary_key": "id_jurnal, id_akun"
    },
    "transaksi": {
      "id_transaksi": "primary key",
      "tanggal": "date",
      "jenis_transaksi": "varchar(50)",
      "jumlah_total": "decimal(18,2)",
      "keterangan": "text",
      "id_jurnal": "foreign key references jurnal_umum(id_jurnal)"
    }
  }
}

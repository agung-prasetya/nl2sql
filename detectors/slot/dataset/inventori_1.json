{
  "entitas": {
    "barang": {
      "id_barang": "PRIMARY KEY",
      "nama_barang": "VARCHAR(100)",
      "kategori": "VARCHAR(50)",
      "satuan": "VARCHAR(20)",
      "stok": "INT",
      "harga_satuan": "DECIMAL(10,2)"
    },
    "pemasok": {
      "id_pemasok": "PRIMARY KEY",
      "nama_pemasok": "VARCHAR(100)",
      "alamat": "TEXT",
      "telepon": "VARCHAR(20)"
    },
    "barang_masuk": {
      "id_masuk": "PRIMARY KEY",
      "id_barang": "FOREIGN KEY REFERENCES barang(id_barang)",
      "id_pemasok": "FOREIGN KEY REFERENCES pemasok(id_pemasok)",
      "tanggal_masuk": "DATE",
      "jumlah": "INT",
      "keterangan": "TEXT"
    },
    "barang_keluar": {
      "id_keluar": "PRIMARY KEY",
      "id_barang": "FOREIGN KEY REFERENCES barang(id_barang)",
      "tanggal_keluar": "DATE",
      "jumlah": "INT",
      "tujuan": "VARCHAR(100)",
      "keterangan": "TEXT"
    }
  }
}

{
  "entitas": {
    "barang": {
      "id_barang": "primary key",
      "nama_barang": "varchar(100)",
      "id_kategori": "foreign key references kategori(id_kategori)",
      "jumlah": "integer",
      "harga_satuan": "decimal(12,2)",
      "satuan": "varchar(20)",
      "id_lokasi": "foreign key references lokasi(id_lokasi)"
    },
    "kategori": {
      "id_kategori": "primary key",
      "nama_kategori": "varchar(50)"
    },
    "pemasok": {
      "id_pemasok": "primary key",
      "nama_pemasok": "varchar(100)",
      "alamat": "varchar(255)",
      "telepon": "varchar(20)"
    },
    "pembelian": {
      "id_pembelian": "primary key",
      "tanggal": "date",
      "id_pemasok": "foreign key references pemasok(id_pemasok)"
    },
    "detail_pembelian": {
      "id_pembelian": "foreign key references pembelian(id_pembelian)",
      "id_barang": "foreign key references barang(id_barang)",
      "jumlah": "integer",
      "harga_beli": "decimal(12,2)",
      "primary_key": "id_pembelian, id_barang"
    },
    "pengeluaran": {
      "id_pengeluaran": "primary key",
      "tanggal": "date",
      "tujuan": "varchar(100)"
    },
    "detail_pengeluaran": {
      "id_pengeluaran": "foreign key references pengeluaran(id_pengeluaran)",
      "id_barang": "foreign key references barang(id_barang)",
      "jumlah": "integer",
      "primary_key": "id_pengeluaran, id_barang"
    },
    "lokasi": {
      "id_lokasi": "primary key",
      "nama_lokasi": "varchar(100)",
      "keterangan": "varchar(255)"
    }
  }
}

    "supplier": {
      "atribut": {
        "id_supplier": "PRIMARY KEY",
        "nama_supplier": "VARCHAR",
        "telepon": "VARCHAR",
        "alamat": "VARCHAR"
      }
    },
    "kendaraan": {
      "atribut": {
        "nopol": "PRIMARY KEY",
        "nama_kendaraan": "VARCHAR",
        "pengemudi": "VARCHAR"
      }
    }
  }
}

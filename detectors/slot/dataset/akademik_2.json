{
  "entitas": {
    "siswa": {
      "id_siswa": "primary key",
      "nama": "varchar(100)",
      "tanggal_lahir": "date",
      "kelas": "varchar(10)"
    },
    "guru": {
      "id_guru": "primary key",
      "nama": "varchar(100)",
      "jabatan": "varchar(50)",
      "status": "varchar(20)"
    },
    "mata_pelajaran": {
      "id_mapel": "primary key",
      "nama_mapel": "varchar(100)",
      "kode_mapel": "varchar(20)"
    },
    "admin": {
      "id_admin": "primary key",
      "nama": "varchar(100)",
      "username": "varchar(50)",
      "password": "varchar(50)"
    },
    "rencana_studi": {
      "id_rencana": "primary key",
      "id_siswa": "foreign key references siswa(id_siswa)",
      "id_mapel": "foreign key references mata_pelajaran(id_mapel)",
      "id_guru": "foreign key references guru(id_guru)",
      "id_admin": "foreign key references admin(id_admin)"
    }
  }
}

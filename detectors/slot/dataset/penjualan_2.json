{
  "entitas": {
    "konsumen": {
      "id_konsumen": "PRIMARY KEY",
      "nama": "VARCHAR(50)",
      "alamat": "VARCHAR(100)",
      "no_telepon": "VARCHAR(20)",
      "email": "VARCHAR(50)",
      "password": "VARCHAR(50)"
    },
    "pemesanan": {
      "id_pemesanan": "PRIMARY KEY",
      "id_konsumen": "FOREIGN KEY REFERENCES konsumen(id_konsumen)",
      "tgl_pesan": "VARCHAR(15)",
      "tgl_bayar": "VARCHAR(15)",
      "no_rekening": "VARCHAR(20)",
      "status": "VARCHAR(20)"
    },
    "detail_pesanan": {
      "id_konsumen": "FOREIGN KEY REFERENCES konsumen(id_konsumen)",
      "id_barang": "FOREIGN KEY REFERENCES barang(id_barang)",
      "id_pemesanan": "FOREIGN KEY REFERENCES pemesanan(id_pemesanan)",
      "total": "VARCHAR(50)"
    },
    "barang": {
      "id_barang": "PRIMARY KEY",
      "nama_barang": "VARCHAR(25)",
      "harga_jual": "VARCHAR(20)",
      "kategori": "VARCHAR(20)",
      "stok": "VARCHAR(100)"
    },
    "admin": {
      "id_admin": "PRIMARY KEY",
      "email": "VARCHAR(50)",
      "password": "VARCHAR(50)"
    },
    "laporan_bulanan": {
      "id_laporanbulanan": "PRIMARY KEY",
      "hasil_penjualan": "VARCHAR(100)",
      "laba_rugi": "VARCHAR(100)"
    },
    "supplier": {
      "id_supplier": "PRIMARY KEY",
      "nama_supplier": "VARCHAR(20)",
      "no_telepon": "VARCHAR(20)",
      "produk": "VARCHAR(100)",
      "alamat": "VARCHAR(100)"
    },
    "owner": {
      "id_owner": "PRIMARY KEY",
      "id_laporanbulanan": "FOREIGN KEY REFERENCES laporan_bulanan(id_laporanbulanan)"
    }
  }
}
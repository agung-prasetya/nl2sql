{
  "entitas": {
    "akun": {
      "id_akun": "primary key",
      "kode_akun": "varchar(20)",
      "nama_akun": "varchar(100)",
      "jenis_akun": "varchar(50)",
      "saldo_awal": "decimal(18,2)",
      "saldo_akhir": "decimal(18,2)"
    },
    "jurnal": {
      "id_jurnal": "primary key",
      "tanggal": "date",
      "keterangan": "text",
      "jenis_jurnal": "varchar(50)"
    },
    "detail_jurnal": {
      "id_detail": "primary key",
      "id_jurnal": "foreign key references jurnal(id_jurnal)",
      "id_akun": "foreign key references akun(id_akun)",
      "debit": "decimal(18,2)",
      "kredit": "decimal(18,2)"
    },
    "transaksi": {
      "id_transaksi": "primary key",
      "tanggal": "date",
      "jenis_transaksi": "varchar(50)",
      "nomor_bukti": "varchar(50)",
      "keterangan": "text",
      "id_jurnal": "foreign key references jurnal(id_jurnal)"
    },
    "pengguna": {
      "id_pengguna": "primary key",
      "nama": "varchar(100)",
      "username": "varchar(50)",
      "password": "varchar(50)",
      "peran": "varchar(20)"
    },
    "periode_akuntansi": {
      "id_periode": "primary key",
      "nama_periode": "varchar(50)",
      "tanggal_mulai": "date",
      "tanggal_selesai": "date",
      "status": "varchar(20)"
    }
  }
}

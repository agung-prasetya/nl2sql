{
  "entitas": {
    "tamu": {
      "id_tamu": "primary key",
      "nama": "string",
      "alamat": "string",
      "no_telepon": "string",
      "email": "string"
    },
    "kamar": {
      "id_kamar": "primary key",
      "nomor_kamar": "string",
      "tipe": "string",
      "harga_per_malam": "float",
      "status": "string"
    },
    "reservasi": {
      "id_reservasi": "primary key",
      "id_tamu": "foreign key → tamu.id_tamu",
      "id_kamar": "foreign key → kamar.id_kamar",
      "tanggal_checkin": "date",
      "tanggal_checkout": "date",
      "status_pembayaran": "string"
    },
    "layanan": {
      "id_layanan": "primary key",
      "nama_layanan": "string",
      "harga": "float"
    },
    "penggunaan_layanan": {
      "id_penggunaan": "primary key",
      "id_reservasi": "foreign key → reservasi.id_reservasi",
      "id_layanan": "foreign key → layanan.id_layanan",
      "tanggal_penggunaan": "date",
      "jumlah": "integer"
    },
    "staf": {
      "id_staf": "primary key",
      "nama": "string",
      "jabatan": "string",
      "no_telepon": "string"
    }
  }
}

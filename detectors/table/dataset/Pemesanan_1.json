{
  "entitas": {
    "pelanggan": {
      "id_pelanggan": "primary key",
      "nama": "string",
      "alamat": "string",
      "no_telepon": "string",
      "email": "string"
    },
    "pesanan": {
      "id_pesanan": "primary key",
      "tanggal_pesan": "date",
      "total_harga": "decimal",
      "id_pelanggan": "foreign key"
    },
    "detail_pesanan": {
      "id_detail": "primary key",
      "id_pesanan": "foreign key",
      "id_produk": "foreign key",
      "jumlah": "integer",
      "harga_satuan": "decimal"
    },
    "produk": {
      "id_produk": "primary key",
      "nama_produk": "string",
      "deskripsi": "string",
      "stok": "integer",
      "harga": "decimal"
    },
    "pembayaran": {
      "id_pembayaran": "primary key",
      "id_pesanan": "foreign key",
      "tanggal_bayar": "date",
      "metode_pembayaran": "string",
      "jumlah_bayar": "decimal",
      "status_bayar": "string"
    }
  }
}
